APP_NAME=Laravel
APP_ENV=production
APP_KEY=
APP_DEBUG=false
# CRITICAL: APP_URL must match your actual Azure App Service URL exactly
# This is used by Vite to generate correct asset URLs
# Example: https://amazinginventory-app.azurewebsites.net
APP_URL=https://your-app-name.azurewebsites.net

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration - Supabase PostgreSQL
# Get these from Supabase Dashboard > Project Settings > Database
DB_CONNECTION=pgsql
DB_HOST=db.your-project-ref.supabase.co
DB_PORT=5432
DB_DATABASE=postgres
DB_USERNAME=postgres
DB_PASSWORD=your_supabase_database_password
# Supabase uses SSL by default
DB_SSLMODE=require

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
# For Supabase: Use 'supabase' for Supabase Storage or 'local' for local storage
FILESYSTEM_DISK=supabase
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS S3 Configuration (if using AWS)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Supabase Configuration
# Get these from Supabase Dashboard > Project Settings > API
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_PROJECT_REF=your-project-ref

# Supabase Storage Configuration (S3-compatible API)
# Get these from Supabase Dashboard > Storage > Settings
# Note: Supabase Storage S3 API credentials are available in Project Settings > API
# For S3-compatible access, you may need to use the service role key or configure S3 credentials
SUPABASE_STORAGE_ACCESS_KEY=your_supabase_storage_access_key
SUPABASE_STORAGE_SECRET_KEY=your_supabase_storage_secret_key
SUPABASE_STORAGE_BUCKET=laravel-storage
SUPABASE_STORAGE_ENDPOINT=https://your-project-ref.supabase.co/storage/v1/s3
SUPABASE_STORAGE_URL=https://your-project-ref.supabase.co/storage/v1/object/public/laravel-storage
SUPABASE_STORAGE_REGION=us-east-1

VITE_APP_NAME="${APP_NAME}"

# Filament Admin Credentials
FILAMENT_ADMIN_EMAIL=admin@example.com
FILAMENT_ADMIN_PASSWORD=password
FILAMENT_ADMIN_NAME=Admin User

# Azure App Service Configuration (for hosting only)
# These are typically set automatically by Azure, but can be overridden
WEBSITE_SITE_NAME=your-app-name
WEBSITE_INSTANCE_ID=

# Supabase Redis (if using Supabase Redis addon)
# Or use Azure Redis Cache if preferred
REDIS_HOST=your-redis-host
REDIS_PASSWORD=your-redis-password
REDIS_PORT=6379
REDIS_CLIENT=phpredis

# Mail Configuration for Azure (using SendGrid or other)
MAIL_MAILER=smtp
MAIL_HOST=smtp.sendgrid.net
MAIL_PORT=587
MAIL_USERNAME=apikey
MAIL_PASSWORD=your_sendgrid_api_key
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@yourdomain.com
MAIL_FROM_NAME="${APP_NAME}"

# Swagger/OpenAPI Documentation
# Set to false in production
L5_SWAGGER_GENERATE_ALWAYS=false
